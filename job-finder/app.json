{
  "expo": {
    "name": "job-finder",
    "slug": "job-finder",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "job-finder",
      "config": {
        "googleMaps": {
          "apiKey": "API_KEY_HERE"
        }
      }
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
